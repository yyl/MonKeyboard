# Keep classes and methods that have the @UsedForTesting annotation
-keep @com.yyl.inputmethod.annotations.UsedForTesting class *
-keepclassmembers class * {
@com.yyl.inputmethod.annotations.UsedForTesting *;
}

# Keep classes and methods that have the @ExternallyReferenced annotation
-keep @com.yyl.inputmethod.annotations.ExternallyReferenced class *
-keepclassmembers class * {
@com.yyl.inputmethod.annotations.ExternallyReferenced *;
}
